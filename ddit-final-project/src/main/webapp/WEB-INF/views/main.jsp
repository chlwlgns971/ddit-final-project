function fn_getNation(event) {
// 	console.log(event.target.value);
	let contiCode = event.target.value;
	console.log('contiCOde : ',contiCode);
	$.ajax({
		url : "${cPath}/nationList?_csrf="+token,
		method : "post",
// 		beforeSend : function(xhr) {
